<#@ modelType: kotlin.collections.List<com.luggsoft.k4.example.Person> #>
<div>
    <#! for (person in model) { #>
        <h1><#= model.lastName #>, <#= model.firstName #></h1>
        <#! if (model.age >= 18) { #>
            <p>You're an adult!</p>
        <#! } else { #>
            <p>You're not an adult yet!</p>
        <#! } #>
    <#! } #>
</div>
